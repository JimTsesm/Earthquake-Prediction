---unzip file to csv
import zipfile
with zipfile.ZipFile("/content/data/train.csv.zip","r") as zip_ref:
    zip_ref.extractall("/content/data/earthquake")

import csv
with open('/content/data/earthquake/train.csv.zip') as csvfile:
    reader = csv.DictReader(csvfile)
    for i,row in enumerate(reader):
        print(row['acoustic_data'], row['time_to_failure'])
        if(i >= 9):
            break